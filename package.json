{
  "name": "buttcaster",
  "version": "32.0.0",
  "private": true,
  "description": "ButtCaster \u2014 OBS overlay + Intiface/Buttplug haptics with adult-platform adapters",
  "main": "desktop/main.js",
  "scripts": {
    "dev": "node server/index.js",
    "start": "npx electron desktop/main.js",
    "build": "npx electron-builder -c.extraMetadata.main=desktop/main.js"
  },
  "dependencies": {
    "express": "^4.19.2",
    "socket.io": "^4.7.5",
    "buttplug": "^3.2.2",
    "ws": "^8.18.0",
    "cors": "^2.8.5",
    "electron": "^31.2.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.buttcaster.app",
    "productName": "ButtCaster",
    "win": {
      "target": "nsis"
    },
    "files": [
      "server/**/*",
      "web/**/*",
      "desktop/**/*",
      "package.json",
      "README.md",
      "LICENSE"
    ]
  }
}